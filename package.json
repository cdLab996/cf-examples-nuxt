{
  "name": "@cdlab996/cf-examples-nuxt",
  "type": "module",
  "version": "0.4.4",
  "description": "A simple, efficient URL shortener using Nuxt, TypeScript, and Cloudflare Pages with D1 Database integration.",
  "author": "wudi",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "clean": "bash ./scripts/clean.sh",
    "generate": "nuxt generate",
    "preview": "pnpm run build && wrangler pages dev",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "deploy": "pnpm run build && wrangler pages deploy",
    "cf-typegen": "wrangler types",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:up": "drizzle-kit up",
    "db:introspect": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "db:check": "drizzle-kit check",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check --write .",
    "format:fail": "prettier --check ."
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "better-sqlite3": "^11.2.1",
    "dayjs": "^1.11.13",
    "db0": "^0.1.4",
    "drizzle-kit": "^0.24.1",
    "drizzle-orm": "^0.33.0",
    "element-plus": "^2.8.0",
    "nanoid": "^5.0.7",
    "nuxt": "^3.13.0",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cdlab996/eslint-config-vue": "0.0.13",
    "@cdlab996/prettier-config": "0.0.13",
    "@cloudflare/workers-types": "^4.20240821.1",
    "@element-plus/nuxt": "^1.0.9",
    "@unocss/nuxt": "^0.62.2",
    "@vueuse/nuxt": "^11.0.1",
    "eslint": "8.57.0",
    "nitro-cloudflare-dev": "^0.1.6",
    "prettier": "3.3.1",
    "unocss": "^0.62.2",
    "vue-tsc": "latest",
    "wrangler": "^3.72.2"
  }
}
